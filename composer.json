{
    "name":"someshwer/version-comparison",
    "description":"This Laravel package compares two version strings and gives the Boolean result. This package also resolves version expressions like (($v > 1.24.0) && ($v < 1.25.1.0)) || ($v == 1.26 || $v == 1.27) where $v must be substituted with the version number to be compared. Hence the package can be used for version expressions evaluation.",
    "type":"library",
    "keywords":[
       "versions",
       "version-comparison",
       "comparator",
       "comparison",
       "version-comparator",
       "evaluation",
       "expression",
       "evaluations",
       "expressions",
       "expression-evaluation"
    ],
    "homepage":"https://github.com/bsomeshwer/version-comparison",
    "license":"MIT",
    "authors":[
       {
          "name":"Someshwer Bandapally",
          "email":"bsomeshwer89@gmail.com"
       }
    ],
    "minimum-stability":"stable",
    "support":{
       "issues":"https://github.com/bsomeshwer/version-comparison/issues",
       "source":"https://github.com/bsomeshwer/version-comparison.git"
    },
    "require":{
       "php":"^5.4 || ^7.0",
       "composer/semver": "^1.4",
       "symfony/expression-language": "^4.1"
    },
    "require-dev":{
       "phpunit/phpunit":"^4.7|>=5.0 <5.4",
       "composer/semver": "^1.4",
       "symfony/expression-language": "^4.1"
    },
    "autoload":{
       "psr-4":{
          "Someshwer\\VersionComparison\\":"src/"
       }
    },
    "autoload-dev":{
       "psr-4":{
          "Someshwer\\VersionComparison\\Tests\\":"src/tests"
       }
    }
 }
